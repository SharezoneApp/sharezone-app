{
    "hosting": [
        {
            "target": "alpha-web-app",
            "public": "build/web",
            "ignore": [
                "firebase.json",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "headers": [
                {
                    "source": "/.well-known/assetlinks.json",
                    "headers": [
                        {
                            "key": "Content-Type",
                            "value": "application/json"
                        }
                    ]
                },
                {
                    "source": "/.well-known/apple-app-site-association",
